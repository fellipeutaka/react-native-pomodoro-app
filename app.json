{
  "expo": {
    "name": "Pomodora",
    "slug": "pomodora",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#181a20"
    },
    "updates": {
      "fallbackToCacheTimeout": 0

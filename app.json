    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "android": {
      "package": "com.fellipeutaka.pomodora"
    }
  }
}
